// use of super keyword explicitly

class Parent {
    
    Parent(int i) {
        System.out.println("Parent constructor");
    }
}

class Sub extends Parent {
    
    Sub() {
    
    super(6);
        System.out.println("SubClass");
    }
    void display() {
        System.out.println("Hello");
    
}
}

public class Main {
    public static void main(String args []) {
        Sub ob = new Sub();
        
        ob.display();
    }
}
